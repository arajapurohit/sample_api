<%= form_for @school,:url => test_index_path, :method =>'post' do |f| %>

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_area :email %>
  </p>

  <p>
    <%= f.label :phone %><br>
    <%= f.text_area :phone %>
  </p>

  <p>
    <%= f.label :address %><br>
    <%= f.text_area :address %>
  </p>

  <p>
    <%= f.label :status %><br>
    <%= f.text_area :status %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>